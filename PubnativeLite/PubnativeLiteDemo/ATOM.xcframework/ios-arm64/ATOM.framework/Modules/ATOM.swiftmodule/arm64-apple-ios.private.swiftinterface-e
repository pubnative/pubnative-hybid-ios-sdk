extension Swift.Int : ATOM.DatabaseValueConvertible, ATOM.StatementColumnConvertible {
  @inlinable public init(sqliteStatement: ATOM.SQLiteStatement, index: Swift.Int32) {
        let int64 = sqlite3_column_int64(sqliteStatement, index)
        if let v = Int(exactly: int64) {
            self = v
        } else {
            fatalConversionError(to: Int.self, sqliteStatement: sqliteStatement, index: index)
        }
    }
  public var databaseValue: ATOM.DatabaseValue {
    get
  }
  public static func fromDatabaseValue(_ dbValue: ATOM.DatabaseValue) -> Swift.Int?
}
