  convenience public init()
  convenience public init(_ dictionary: [Swift.String : (any ATOM.DatabaseValueConvertible)?])
  convenience public init?(_ dictionary: [Swift.AnyHashable : Any])
  final public func copy() -> ATOM.Row
  public typealias Element = (Swift.String, ATOM.DatabaseValue)
  public typealias Index = ATOM.RowIndex
  public typealias Indices = Swift.Range<ATOM.RowIndex>
  public typealias Iterator = Swift.IndexingIterator<ATOM.Row>
  public typealias Key = Swift.String
  public typealias SubSequence = Swift.Slice<ATOM.Row>
  public typealias Value = (any ATOM.DatabaseValueConvertible)?
  @objc deinit
  final public var hashValue: Swift.Int {
    get
  }
}
extension ATOM.Row {
  final public var columnNames: Swift.LazyMapCollection<ATOM.Row, Swift.String> {
