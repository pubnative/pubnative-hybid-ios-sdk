  public static func combine<R1, R2, R3, R4, R5>(_ o1: ATOM.ValueObservation<R1>, _ o2: ATOM.ValueObservation<R2>, _ o3: ATOM.ValueObservation<R3>, _ o4: ATOM.ValueObservation<R4>, _ o5: ATOM.ValueObservation<R5>) -> ATOM.ValueObservation<ATOM.ValueReducers.Combine5<R1, R2, R3, R4, R5>> where R1 : ATOM.ValueReducer, R2 : ATOM.ValueReducer, R3 : ATOM.ValueReducer, R4 : ATOM.ValueReducer, R5 : ATOM.ValueReducer
