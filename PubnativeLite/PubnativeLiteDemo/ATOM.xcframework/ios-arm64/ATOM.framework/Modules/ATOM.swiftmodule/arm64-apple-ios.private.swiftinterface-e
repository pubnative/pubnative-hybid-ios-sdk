  public func group(_ expressions: [any ATOM.SQLExpressible]) -> Self
  public func group(_ expressions: any ATOM.SQLExpressible...) -> Self
  public func group(sql: Swift.String, arguments: ATOM.StatementArguments = StatementArguments()) -> Self
  public func group(literal sqlLiteral: ATOM.SQLLiteral) -> Self
  public func having(sql: Swift.String, arguments: ATOM.StatementArguments = StatementArguments()) -> Self
  public func having(literal sqlLiteral: ATOM.SQLLiteral) -> Self
}
public protocol OrderedRequest {
  func order(_ orderings: @escaping (ATOM.Database) throws -> [any ATOM.SQLOrderingTerm]) -> Self
  func reversed() -> Self
  func unordered() -> Self
}
