extension ATOM.ValueObservation where Reducer == () {
  @available(*, deprecated, message: "Use request.observationForAll() instead")
  public static func trackingAll<Request>(_ request: Request) -> ATOM.ValueObservation<ATOM.FetchableRecordsReducer<Request.RowDecoder>> where Request : ATOM.FetchRequest, Request.RowDecoder : ATOM.FetchableRecord
  @available(*, deprecated, message: "Use request.observationForFirst() instead")
  public static func trackingOne<Request>(_ request: Request) -> ATOM.ValueObservation<ATOM.FetchableRecordReducer<Request.RowDecoder>> where Request : ATOM.FetchRequest, Request.RowDecoder : ATOM.FetchableRecord
}
extension ATOM.ValueReducers {
  public struct AllRecords<RowDecoder> : ATOM.ValueReducer where RowDecoder : ATOM.FetchableRecord {
    public func fetch(_ db: ATOM.Database) throws -> [ATOM.Row]
    public mutating func value(_ rows: [ATOM.Row]) -> [RowDecoder]?
    public typealias Fetched = [ATOM.Row]
    public typealias Value = [RowDecoder]
  }
  public struct OneRecord<RowDecoder> : ATOM.ValueReducer where RowDecoder : ATOM.FetchableRecord {
    public func fetch(_ db: ATOM.Database) throws -> ATOM.Row?
    public mutating func value(_ row: ATOM.Row?) -> RowDecoder??
    public typealias Fetched = ATOM.Row?
    public typealias Value = RowDecoder?
  }
}
@available(*, deprecated, renamed: "ValueReducers.AllRecords")
public typealias FetchableRecordsReducer<RowDecoder> = ATOM.ValueReducers.AllRecords<RowDecoder> where RowDecoder : ATOM.FetchableRecord
@available(*, deprecated, renamed: "ValueReducers.OneRecord")
