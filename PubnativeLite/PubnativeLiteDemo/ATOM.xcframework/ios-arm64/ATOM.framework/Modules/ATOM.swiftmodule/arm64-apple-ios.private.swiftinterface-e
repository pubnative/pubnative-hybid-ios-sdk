    public typealias Value = (R1.Value, R2.Value, R3.Value, R4.Value, R5.Value)
    public func fetch(_ db: ATOM.Database) throws -> ATOM.ValueReducers.Combine5<R1, R2, R3, R4, R5>.Fetched
    public mutating func value(_ fetched: ATOM.ValueReducers.Combine5<R1, R2, R3, R4, R5>.Fetched) -> ATOM.ValueReducers.Combine5<R1, R2, R3, R4, R5>.Value?
  }
}
