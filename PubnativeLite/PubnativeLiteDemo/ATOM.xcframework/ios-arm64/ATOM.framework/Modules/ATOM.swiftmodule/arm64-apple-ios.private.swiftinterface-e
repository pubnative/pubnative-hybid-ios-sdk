  public func order(_ orderings: any ATOM.SQLOrderingTerm...) -> Self
  public func order(_ orderings: [any ATOM.SQLOrderingTerm]) -> Self
  public func order(sql: Swift.String, arguments: ATOM.StatementArguments = StatementArguments()) -> Self
  public func order(literal sqlLiteral: ATOM.SQLLiteral) -> Self
}
public protocol _JoinableRequest {
  func _including(all association: ATOM.SQLAssociation) -> Self
  func _including(optional association: ATOM.SQLAssociation) -> Self
  func _including(required association: ATOM.SQLAssociation) -> Self
  func _joining(optional association: ATOM.SQLAssociation) -> Self
  func _joining(required association: ATOM.SQLAssociation) -> Self
}
public protocol JoinableRequest : ATOM._JoinableRequest {
  associatedtype RowDecoder : ATOM.TableRecord
}
extension ATOM.JoinableRequest {
  public func including<A>(all association: A) -> Self where A : ATOM.AssociationToMany, Self.RowDecoder == A.OriginRowDecoder
  public func including<A>(optional association: A) -> Self where A : ATOM.Association, Self.RowDecoder == A.OriginRowDecoder
  public func including<A>(required association: A) -> Self where A : ATOM.Association, Self.RowDecoder == A.OriginRowDecoder
  public func joining<A>(optional association: A) -> Self where A : ATOM.Association, Self.RowDecoder == A.OriginRowDecoder
  public func joining<A>(required association: A) -> Self where A : ATOM.Association, Self.RowDecoder == A.OriginRowDecoder
}
