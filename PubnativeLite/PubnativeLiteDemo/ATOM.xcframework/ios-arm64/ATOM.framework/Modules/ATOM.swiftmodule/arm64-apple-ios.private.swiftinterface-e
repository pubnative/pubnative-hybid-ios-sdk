  final public func unsafeRead<T>(_ block: (ATOM.Database) throws -> T) throws -> T
  final public func unsafeReentrantRead<T>(_ block: (ATOM.Database) throws -> T) throws -> T
  final public func concurrentRead<T>(_ block: @escaping (ATOM.Database) throws -> T) -> ATOM.DatabaseFuture<T>
  final public func spawnConcurrentRead(_ block: @escaping (Swift.Result<ATOM.Database, any Swift.Error>) -> Swift.Void)
  final public func asyncConcurrentRead(_ block: @escaping (Swift.Result<ATOM.Database, any Swift.Error>) -> Swift.Void)
  final public func invalidateReadOnlyConnections()
  final public func writeWithoutTransaction<T>(_ updates: (ATOM.Database) throws -> T) rethrows -> T
  final public func barrierWriteWithoutTransaction<T>(_ updates: (ATOM.Database) throws -> T) rethrows -> T
  final public func writeInTransaction(_ kind: ATOM.Database.TransactionKind? = nil, _ updates: (ATOM.Database) throws -> ATOM.Database.TransactionCompletion) throws
  final public func unsafeReentrantWrite<T>(_ updates: (ATOM.Database) throws -> T) rethrows -> T
  final public func asyncWriteWithoutTransaction(_ updates: @escaping (ATOM.Database) -> Swift.Void)
  final public func add(function: ATOM.DatabaseFunction)
  final public func remove(function: ATOM.DatabaseFunction)
  final public func add(collation: ATOM.DatabaseCollation)
  final public func remove(collation: ATOM.DatabaseCollation)
}
extension ATOM.DatabasePool {
  final public func makeSnapshot() throws -> ATOM.DatabaseSnapshot
}
extension ATOM.FetchableRecord where Self : ATOM.TableRecord {
  public static func fetchCursor(_ db: ATOM.Database) throws -> ATOM.RecordCursor<Self>
  public static func fetchAll(_ db: ATOM.Database) throws -> [Self]
  public static func fetchOne(_ db: ATOM.Database) throws -> Self?
}
extension ATOM.FetchableRecord where Self : ATOM.TableRecord {
  public static func fetchCursor<Sequence>(_ db: ATOM.Database, keys: Sequence) throws -> ATOM.RecordCursor<Self> where Sequence : Swift.Sequence, Sequence.Element : ATOM.DatabaseValueConvertible
  public static func fetchAll<Sequence>(_ db: ATOM.Database, keys: Sequence) throws -> [Self] where Sequence : Swift.Sequence, Sequence.Element : ATOM.DatabaseValueConvertible
  public static func fetchOne<PrimaryKeyType>(_ db: ATOM.Database, key: PrimaryKeyType?) throws -> Self? where PrimaryKeyType : ATOM.DatabaseValueConvertible
}
extension ATOM.FetchableRecord where Self : ATOM.TableRecord {
  public static func fetchCursor(_ db: ATOM.Database, keys: [[Swift.String : (any ATOM.DatabaseValueConvertible)?]]) throws -> ATOM.RecordCursor<Self>
  public static func fetchAll(_ db: ATOM.Database, keys: [[Swift.String : (any ATOM.DatabaseValueConvertible)?]]) throws -> [Self]
  public static func fetchOne(_ db: ATOM.Database, key: [Swift.String : (any ATOM.DatabaseValueConvertible)?]?) throws -> Self?
}
public protocol SelectionRequest {
  func select(_ selection: [any ATOM.SQLSelectable]) -> Self
  func annotated(with selection: [any ATOM.SQLSelectable]) -> Self
}
extension ATOM.SelectionRequest {
  public func select(_ selection: any ATOM.SQLSelectable...) -> Self
  public func select(sql: Swift.String, arguments: ATOM.StatementArguments = StatementArguments()) -> Self
  public func select(literal sqlLiteral: ATOM.SQLLiteral) -> Self
  public func annotated(with selection: any ATOM.SQLSelectable...) -> Self
}
public protocol FilteredRequest {
  func filter(_ predicate: @escaping (ATOM.Database) throws -> any ATOM.SQLExpressible) -> Self
  func expectingSingleResult() -> Self
}
extension ATOM.FilteredRequest {
  public func expectingSingleResult() -> Self
  public func filter(_ predicate: any ATOM.SQLExpressible) -> Self
  public func filter(sql: Swift.String, arguments: ATOM.StatementArguments = StatementArguments()) -> Self
  public func filter(literal sqlLiteral: ATOM.SQLLiteral) -> Self
  public func none() -> Self
}
public protocol TableRequest {
  var databaseTableName: Swift.String { get }
  func aliased(_ alias: ATOM.TableAlias) -> Self
}
