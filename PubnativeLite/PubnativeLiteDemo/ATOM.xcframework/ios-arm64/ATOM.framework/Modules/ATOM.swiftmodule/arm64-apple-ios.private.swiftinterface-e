extension Swift.Array {
  @inlinable public init<C>(_ cursor: C) throws where Element == C.Element, C : ATOM.Cursor {
        self.init()
        while let element = try cursor.next() {
            append(element)
        }
    }
}
extension Swift.Sequence {
  public func flatMap<SegmentOfResult>(_ transform: @escaping (Self.Iterator.Element) throws -> SegmentOfResult) -> ATOM.FlattenCursor<ATOM.MapCursor<ATOM.AnyCursor<Self.Iterator.Element>, SegmentOfResult>> where SegmentOfResult : ATOM.Cursor
}
extension Swift.Set {
  public init<C>(_ cursor: C) throws where Element == C.Element, C : ATOM.Cursor
}
public protocol Cursor : AnyObject {
  associatedtype Element
  func next() throws -> Self.Element?
}
extension ATOM.Cursor {
  public func isEmpty() throws -> Swift.Bool
  public func contains(where predicate: (Self.Element) throws -> Swift.Bool) throws -> Swift.Bool
  public func enumerated() -> ATOM.EnumeratedCursor<Self>
  public func filter(_ isIncluded: @escaping (Self.Element) throws -> Swift.Bool) -> ATOM.FilterCursor<Self>
  public func first(where predicate: (Self.Element) throws -> Swift.Bool) throws -> Self.Element?
  public func compactMap<ElementOfResult>(_ transform: @escaping (Self.Element) throws -> ElementOfResult?) -> ATOM.MapCursor<ATOM.FilterCursor<ATOM.MapCursor<Self, ElementOfResult?>>, ElementOfResult>
  public func drop(while predicate: @escaping (Self.Element) throws -> Swift.Bool) -> ATOM.DropWhileCursor<Self>
  public func dropFirst(_ n: Swift.Int) -> ATOM.DropFirstCursor<Self>
  public func dropFirst() -> ATOM.DropFirstCursor<Self>
  public func dropLast(_ n: Swift.Int) throws -> [Self.Element]
  public func dropLast() throws -> [Self.Element]
  public func flatMap<SegmentOfResult>(_ transform: @escaping (Self.Element) throws -> SegmentOfResult) -> ATOM.FlattenCursor<ATOM.MapCursor<Self, ATOM.AnyCursor<SegmentOfResult.Element>>> where SegmentOfResult : Swift.Sequence
  public func flatMap<SegmentOfResult>(_ transform: @escaping (Self.Element) throws -> SegmentOfResult) -> ATOM.FlattenCursor<ATOM.MapCursor<Self, SegmentOfResult>> where SegmentOfResult : ATOM.Cursor
  public func forEach(_ body: (Self.Element) throws -> Swift.Void) throws
  public func map<T>(_ transform: @escaping (Self.Element) throws -> T) -> ATOM.MapCursor<Self, T>
  public func max(by areInIncreasingOrder: (Self.Element, Self.Element) throws -> Swift.Bool) throws -> Self.Element?
  public func min(by areInIncreasingOrder: (Self.Element, Self.Element) throws -> Swift.Bool) throws -> Self.Element?
  public func prefix(_ maxLength: Swift.Int) -> ATOM.PrefixCursor<Self>
  public func prefix(while predicate: @escaping (Self.Element) throws -> Swift.Bool) -> ATOM.PrefixWhileCursor<Self>
  public func reduce<Result>(_ initialResult: Result, _ nextPartialResult: (Result, Self.Element) throws -> Result) throws -> Result
  public func reduce<Result>(into initialResult: Result, _ updateAccumulatingResult: (inout Result, Self.Element) throws -> Swift.Void) throws -> Result
  public func suffix(_ maxLength: Swift.Int) throws -> [Self.Element]
}
extension ATOM.Cursor where Self.Element : Swift.Equatable {
  public func contains(_ element: Self.Element) throws -> Swift.Bool
}
extension ATOM.Cursor where Self.Element : Swift.Comparable {
  public func max() throws -> Self.Element?
  public func min() throws -> Self.Element?
}
extension ATOM.Cursor where Self.Element : ATOM.Cursor {
  public func joined() -> ATOM.FlattenCursor<Self>
}
extension ATOM.Cursor where Self.Element : Swift.Sequence {
  public func joined() -> ATOM.FlattenCursor<ATOM.MapCursor<Self, ATOM.AnyCursor<Self.Element.Element>>>
}
extension ATOM.Cursor where Self.Element : Swift.StringProtocol {
  public func joined(separator: Swift.String = "") throws -> Swift.String
}
final public class AnyCursor<Element> : ATOM.Cursor {
  public init<C>(_ base: C) where Element == C.Element, C : ATOM.Cursor
  convenience public init<I>(iterator: I) where Element == I.Element, I : Swift.IteratorProtocol
  convenience public init<S>(_ s: S) where Element == S.Element, S : Swift.Sequence
  public init(_ body: @escaping () throws -> Element?)
  final public func next() throws -> Element?
  @objc deinit
}
