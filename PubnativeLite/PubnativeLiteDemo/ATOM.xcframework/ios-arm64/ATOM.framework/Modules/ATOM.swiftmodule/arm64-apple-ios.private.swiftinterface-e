extension Swift.Bool : ATOM.DatabaseValueConvertible, ATOM.StatementColumnConvertible {
  @inlinable public init(sqliteStatement: ATOM.SQLiteStatement, index: Swift.Int32) {
        self = sqlite3_column_int64(sqliteStatement, index) != 0
    }
