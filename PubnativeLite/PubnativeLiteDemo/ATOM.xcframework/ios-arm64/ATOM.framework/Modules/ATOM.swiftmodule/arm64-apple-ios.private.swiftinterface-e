@_hasMissingDesignatedInitializers final public class RowCursor : ATOM.Cursor {
  final public let statement: ATOM.SelectStatement
  @usableFromInline
  final internal let _sqliteStatement: ATOM.SQLiteStatement
  @usableFromInline
  final internal let _row: ATOM.Row
  @usableFromInline
  final internal var _done: Swift.Bool
  @objc deinit
  @inlinable final public func next() throws -> ATOM.Row? {
        if _done {
            // make sure this instance never yields a value again, even if the
            // statement is reset by another cursor.
            return nil
        }
        switch sqlite3_step(_sqliteStatement) {
        case SQLITE_DONE:
            _done = true
            return nil
        case SQLITE_ROW:
            return _row
        case let code:
            try statement.didFail(withResultCode: code)
        }
    }
  public typealias Element = ATOM.Row
}
extension ATOM.Row {
  public static func fetchCursor(_ statement: ATOM.SelectStatement, arguments: ATOM.StatementArguments? = nil, adapter: (any ATOM.RowAdapter)? = nil) throws -> ATOM.RowCursor
  public static func fetchAll(_ statement: ATOM.SelectStatement, arguments: ATOM.StatementArguments? = nil, adapter: (any ATOM.RowAdapter)? = nil) throws -> [ATOM.Row]
  public static func fetchOne(_ statement: ATOM.SelectStatement, arguments: ATOM.StatementArguments? = nil, adapter: (any ATOM.RowAdapter)? = nil) throws -> ATOM.Row?
}
extension ATOM.Row {
  public static func fetchCursor(_ db: ATOM.Database, sql: Swift.String, arguments: ATOM.StatementArguments = StatementArguments(), adapter: (any ATOM.RowAdapter)? = nil) throws -> ATOM.RowCursor
  public static func fetchAll(_ db: ATOM.Database, sql: Swift.String, arguments: ATOM.StatementArguments = StatementArguments(), adapter: (any ATOM.RowAdapter)? = nil) throws -> [ATOM.Row]
  public static func fetchOne(_ db: ATOM.Database, sql: Swift.String, arguments: ATOM.StatementArguments = StatementArguments(), adapter: (any ATOM.RowAdapter)? = nil) throws -> ATOM.Row?
}
extension ATOM.Row {
  public static func fetchCursor<R>(_ db: ATOM.Database, _ request: R) throws -> ATOM.RowCursor where R : ATOM.FetchRequest
  public static func fetchAll<R>(_ db: ATOM.Database, _ request: R) throws -> [ATOM.Row] where R : ATOM.FetchRequest
  public static func fetchOne<R>(_ db: ATOM.Database, _ request: R) throws -> ATOM.Row? where R : ATOM.FetchRequest
}
