public protocol ValueReducer {
  associatedtype Fetched
  associatedtype Value
  func fetch(_ db: ATOM.Database) throws -> Self.Fetched
  mutating func value(_ fetched: Self.Fetched) -> Self.Value?
}
public struct AnyValueReducer<Fetched, Value> : ATOM.ValueReducer {
  public init(fetch: @escaping (ATOM.Database) throws -> Fetched, value: @escaping (Fetched) -> Value?)
  public init<Base>(_ reducer: Base) where Fetched == Base.Fetched, Value == Base.Value, Base : ATOM.ValueReducer
  public func fetch(_ db: ATOM.Database) throws -> Fetched
  public func value(_ fetched: Fetched) -> Value?
}
