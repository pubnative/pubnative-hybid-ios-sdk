  @objc deinit
}
@_hasMissingDesignatedInitializers final public class ColumnDefinition {
  @discardableResult
  final public func primaryKey(onConflict conflictResolution: ATOM.Database.ConflictResolution? = nil, autoincrement: Swift.Bool = false) -> Self
  @discardableResult
  final public func notNull(onConflict conflictResolution: ATOM.Database.ConflictResolution? = nil) -> Self
  @discardableResult
  final public func unique(onConflict conflictResolution: ATOM.Database.ConflictResolution? = nil) -> Self
  @discardableResult
  final public func indexed() -> Self
  @discardableResult
  final public func check(_ condition: (ATOM.Column) -> any ATOM.SQLExpressible) -> Self
  @discardableResult
  final public func check(sql: Swift.String) -> Self
  @discardableResult
  final public func defaults(to value: any ATOM.DatabaseValueConvertible) -> Self
  @discardableResult
  final public func defaults(sql: Swift.String) -> Self
  @discardableResult
  final public func collate(_ collation: ATOM.Database.CollationName) -> Self
  @discardableResult
  final public func collate(_ collation: ATOM.DatabaseCollation) -> Self
  @discardableResult
  final public func references(_ table: Swift.String, column: Swift.String? = nil, onDelete deleteAction: ATOM.Database.ForeignKeyAction? = nil, onUpdate updateAction: ATOM.Database.ForeignKeyAction? = nil, deferred: Swift.Bool = false) -> Self
  @objc deinit
}
extension ATOM.Database {
  final public func makeSelectStatement(sql: Swift.String) throws -> ATOM.SelectStatement
  final public func cachedSelectStatement(sql: Swift.String) throws -> ATOM.SelectStatement
  final public func makeUpdateStatement(sql: Swift.String) throws -> ATOM.UpdateStatement
  final public func cachedUpdateStatement(sql: Swift.String) throws -> ATOM.UpdateStatement
  final public func execute(sql: Swift.String, arguments: ATOM.StatementArguments = StatementArguments()) throws
  final public func execute(literal sqlLiteral: ATOM.SQLLiteral) throws
}
public protocol DatabaseValueConvertible : ATOM.SQLExpressible {
  var databaseValue: ATOM.DatabaseValue { get }
  static func fromDatabaseValue(_ dbValue: ATOM.DatabaseValue) -> Self?
}
extension ATOM.DatabaseValueConvertible {
  public var sqlExpression: any ATOM.SQLExpression {
    get
  }
}
@_hasMissingDesignatedInitializers final public class DatabaseValueCursor<Value> : ATOM.Cursor where Value : ATOM.DatabaseValueConvertible {
  @usableFromInline
  final internal let _statement: ATOM.SelectStatement
  @usableFromInline
  final internal let _sqliteStatement: ATOM.SQLiteStatement
  @usableFromInline
  final internal let _columnIndex: Swift.Int32
  @usableFromInline
  final internal var _done: Swift.Bool
  @objc deinit
  @inlinable final public func next() throws -> Value? {
        if _done {
            // make sure this instance never yields a value again, even if the
            // statement is reset by another cursor.
            return nil
        }
        switch sqlite3_step(_sqliteStatement) {
        case SQLITE_DONE:
            _done = true
            return nil
        case SQLITE_ROW:
            return Value.decode(from: _sqliteStatement, atUncheckedIndex: _columnIndex)
        case let code:
            try _statement.didFail(withResultCode: code)
        }
    }
  public typealias Element = Value
}
@_hasMissingDesignatedInitializers final public class NullableDatabaseValueCursor<Value> : ATOM.Cursor where Value : ATOM.DatabaseValueConvertible {
  @usableFromInline
  final internal let _statement: ATOM.SelectStatement
  @usableFromInline
  final internal let _sqliteStatement: ATOM.SQLiteStatement
  @usableFromInline
  final internal let _columnIndex: Swift.Int32
  @usableFromInline
  final internal var _done: Swift.Bool
  @objc deinit
  @inlinable final public func next() throws -> Value?? {
        if _done {
            // make sure this instance never yields a value again, even if the
            // statement is reset by another cursor.
            return nil
        }
        switch sqlite3_step(_sqliteStatement) {
        case SQLITE_DONE:
            _done = true
            return nil
        case SQLITE_ROW:
            return Value.decodeIfPresent(from: _sqliteStatement, atUncheckedIndex: _columnIndex)
        case let code:
            try _statement.didFail(withResultCode: code)
        }
    }
  public typealias Element = Value?
}
