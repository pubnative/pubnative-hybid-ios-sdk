extension Foundation.Data : ATOM.DatabaseValueConvertible, ATOM.StatementColumnConvertible {
  @inlinable public init(sqliteStatement: ATOM.SQLiteStatement, index: Swift.Int32) {
        if let bytes = sqlite3_column_blob(sqliteStatement, index) {
            let count = Int(sqlite3_column_bytes(sqliteStatement, index))
            self.init(bytes: bytes, count: count) // copy bytes
        } else {
            self.init()
        }
    }
  public var databaseValue: ATOM.DatabaseValue {
    get
  }
  public static func fromDatabaseValue(_ dbValue: ATOM.DatabaseValue) -> Foundation.Data?
}
