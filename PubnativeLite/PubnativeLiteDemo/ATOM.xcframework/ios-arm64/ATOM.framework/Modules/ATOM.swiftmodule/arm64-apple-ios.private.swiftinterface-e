public protocol DatabaseReader : AnyObject {
  var configuration: ATOM.Configuration { get }
  func interrupt()
  func read<T>(_ block: (ATOM.Database) throws -> T) throws -> T
  func asyncRead(_ block: @escaping (Swift.Result<ATOM.Database, any Swift.Error>) -> Swift.Void)
  func unsafeRead<T>(_ block: (ATOM.Database) throws -> T) throws -> T
  func unsafeReentrantRead<T>(_ block: (ATOM.Database) throws -> T) throws -> T
  func add(function: ATOM.DatabaseFunction)
  func remove(function: ATOM.DatabaseFunction)
  func add(collation: ATOM.DatabaseCollation)
  func remove(collation: ATOM.DatabaseCollation)
  func add<Reducer>(observation: ATOM.ValueObservation<Reducer>, onError: @escaping (any Swift.Error) -> Swift.Void, onChange: @escaping (Reducer.Value) -> Swift.Void) -> any ATOM.TransactionObserver where Reducer : ATOM.ValueReducer
  func remove(transactionObserver: any ATOM.TransactionObserver)
}
extension ATOM.DatabaseReader {
  public func backup(to writer: any ATOM.DatabaseWriter) throws
}
final public class AnyDatabaseReader : ATOM.DatabaseReader {
  public init(_ base: any ATOM.DatabaseReader)
  final public var configuration: ATOM.Configuration {
    get
  }
  final public func interrupt()
  final public func read<T>(_ block: (ATOM.Database) throws -> T) throws -> T
  final public func asyncRead(_ block: @escaping (Swift.Result<ATOM.Database, any Swift.Error>) -> Swift.Void)
  final public func unsafeRead<T>(_ block: (ATOM.Database) throws -> T) throws -> T
  final public func unsafeReentrantRead<T>(_ block: (ATOM.Database) throws -> T) throws -> T
  final public func add(function: ATOM.DatabaseFunction)
  final public func remove(function: ATOM.DatabaseFunction)
  final public func add(collation: ATOM.DatabaseCollation)
  final public func remove(collation: ATOM.DatabaseCollation)
  final public func add<Reducer>(observation: ATOM.ValueObservation<Reducer>, onError: @escaping (any Swift.Error) -> Swift.Void, onChange: @escaping (Reducer.Value) -> Swift.Void) -> any ATOM.TransactionObserver where Reducer : ATOM.ValueReducer
  final public func remove(transactionObserver: any ATOM.TransactionObserver)
  @objc deinit
}
