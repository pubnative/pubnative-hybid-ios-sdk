  final public func create(table name: Swift.String, temporary: Swift.Bool = false, ifNotExists: Swift.Bool = false, body: (ATOM.TableDefinition) -> Swift.Void) throws
  final public func rename(table name: Swift.String, to newName: Swift.String) throws
  final public func alter(table name: Swift.String, body: (ATOM.TableAlteration) -> Swift.Void) throws
  final public func drop(table name: Swift.String) throws
  final public func create(index name: Swift.String, on table: Swift.String, columns: [Swift.String], unique: Swift.Bool = false, ifNotExists: Swift.Bool = false) throws
  @available(macOS 10.10, *)
  final public func create(index name: Swift.String, on table: Swift.String, columns: [Swift.String], unique: Swift.Bool = false, ifNotExists: Swift.Bool = false, condition: any ATOM.SQLExpressible) throws
  final public func drop(index name: Swift.String) throws
  final public func reindex(collation: ATOM.Database.CollationName) throws
  final public func reindex(collation: ATOM.DatabaseCollation) throws
}
@_hasMissingDesignatedInitializers final public class TableDefinition {
  @discardableResult
  final public func autoIncrementedPrimaryKey(_ name: Swift.String, onConflict conflictResolution: ATOM.Database.ConflictResolution? = nil) -> ATOM.ColumnDefinition
  @discardableResult
  final public func column(_ name: Swift.String, _ type: ATOM.Database.ColumnType? = nil) -> ATOM.ColumnDefinition
  final public func primaryKey(_ columns: [Swift.String], onConflict conflictResolution: ATOM.Database.ConflictResolution? = nil)
  final public func uniqueKey(_ columns: [Swift.String], onConflict conflictResolution: ATOM.Database.ConflictResolution? = nil)
  final public func foreignKey(_ columns: [Swift.String], references table: Swift.String, columns destinationColumns: [Swift.String]? = nil, onDelete deleteAction: ATOM.Database.ForeignKeyAction? = nil, onUpdate updateAction: ATOM.Database.ForeignKeyAction? = nil, deferred: Swift.Bool = false)
  final public func check(_ condition: any ATOM.SQLExpressible)
  final public func check(sql: Swift.String)
  @objc deinit
}
