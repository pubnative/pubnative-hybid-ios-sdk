public enum PersistenceError : Swift.Error, Swift.CustomStringConvertible {
  case recordNotFound(databaseTableName: Swift.String, key: [Swift.String : ATOM.DatabaseValue])
}
extension ATOM.PersistenceError {
  public var description: Swift.String {
    get
  }
}
public struct PersistenceConflictPolicy {
  public let conflictResolutionForInsert: ATOM.Database.ConflictResolution
  public let conflictResolutionForUpdate: ATOM.Database.ConflictResolution
  public init(insert: ATOM.Database.ConflictResolution = .abort, update: ATOM.Database.ConflictResolution = .abort)
}
public protocol MutablePersistableRecord : ATOM.EncodableRecord, ATOM.TableRecord {
  static var persistenceConflictPolicy: ATOM.PersistenceConflictPolicy { get }
  mutating func didInsert(with rowID: Swift.Int64, for column: Swift.String?)
  mutating func insert(_ db: ATOM.Database) throws
  func update(_ db: ATOM.Database, columns: Swift.Set<Swift.String>) throws
  mutating func save(_ db: ATOM.Database) throws
  @discardableResult
  func delete(_ db: ATOM.Database) throws -> Swift.Bool
  func exists(_ db: ATOM.Database) throws -> Swift.Bool
}
extension ATOM.MutablePersistableRecord {
  public static var persistenceConflictPolicy: ATOM.PersistenceConflictPolicy {
    get
  }
  public mutating func didInsert(with rowID: Swift.Int64, for column: Swift.String?)
  public mutating func insert(_ db: ATOM.Database) throws
  public func update(_ db: ATOM.Database, columns: Swift.Set<Swift.String>) throws
  public func update<Sequence>(_ db: ATOM.Database, columns: Sequence) throws where Sequence : Swift.Sequence, Sequence.Element : ATOM.ColumnExpression
  public func update<Sequence>(_ db: ATOM.Database, columns: Sequence) throws where Sequence : Swift.Sequence, Sequence.Element == Swift.String
  public func update(_ db: ATOM.Database) throws
  @discardableResult
  public func updateChanges<Record>(_ db: ATOM.Database, from record: Record) throws -> Swift.Bool where Record : ATOM.MutablePersistableRecord
  @discardableResult
  public mutating func updateChanges(_ db: ATOM.Database, with change: (inout Self) throws -> Swift.Void) throws -> Swift.Bool
  public mutating func save(_ db: ATOM.Database) throws
  @discardableResult
  public func delete(_ db: ATOM.Database) throws -> Swift.Bool
  public func exists(_ db: ATOM.Database) throws -> Swift.Bool
  @usableFromInline
  internal func primaryKey(_ db: ATOM.Database) throws -> [Swift.String : ATOM.DatabaseValue]?
  @inlinable public mutating func performInsert(_ db: ATOM.Database) throws {
        let conflictResolutionForInsert = type(of: self).persistenceConflictPolicy.conflictResolutionForInsert
        let dao = try DAO(db, self)
        try dao.insertStatement(onConflict: conflictResolutionForInsert).execute()
        
        if !conflictResolutionForInsert.invalidatesLastInsertedRowID {
            didInsert(with: db.lastInsertedRowID, for: dao.primaryKey.rowIDColumn)
        }
    }
  @inlinable public func performUpdate(_ db: ATOM.Database, columns: Swift.Set<Swift.String>) throws {
        let dao = try DAO(db, self)
        guard
            let statement = try dao.updateStatement(
                columns: columns,
                onConflict: type(of: self).persistenceConflictPolicy.conflictResolutionForUpdate)
            else {
                // Nil primary key
                throw dao.makeRecordNotFoundError()
        }
        try statement.execute()
        if db.changesCount == 0 {
            throw dao.makeRecordNotFoundError()
        }
    }
  @inlinable public mutating func performSave(_ db: ATOM.Database) throws {
        // Call self.insert and self.update so that we support classes that
        // override those methods.
        if let key = try primaryKey(db) {
            do {
                try update(db)
            } catch PersistenceError.recordNotFound(databaseTableName: type(of: self).databaseTableName, key: key) {
                try insert(db)
            }
        } else {
            try insert(db)
        }
    }
  @inlinable public func performDelete(_ db: ATOM.Database) throws -> Swift.Bool {
        guard let statement = try DAO(db, self).deleteStatement() else {
            // Nil primary key
            return false
        }
        try statement.execute()
        return db.changesCount > 0
    }
  @inlinable public func performExists(_ db: ATOM.Database) throws -> Swift.Bool {
        guard let statement = try DAO(db, self).existsStatement() else {
            // Nil primary key
            return false
        }
        return try Row.fetchOne(statement) != nil
    }
}
extension ATOM.MutablePersistableRecord where Self : AnyObject {
  @discardableResult
  public func updateChanges(_ db: ATOM.Database, with change: (Self) throws -> Swift.Void) throws -> Swift.Bool
}
extension ATOM.MutablePersistableRecord {
  @discardableResult
  public static func deleteAll(_ db: ATOM.Database) throws -> Swift.Int
  @discardableResult
  public static func updateAll(_ db: ATOM.Database, onConflict conflictResolution: ATOM.Database.ConflictResolution? = nil, _ assignments: [ATOM.ColumnAssignment]) throws -> Swift.Int
  @discardableResult
  public static func updateAll(_ db: ATOM.Database, onConflict conflictResolution: ATOM.Database.ConflictResolution? = nil, _ assignment: ATOM.ColumnAssignment, _ otherAssignments: ATOM.ColumnAssignment...) throws -> Swift.Int
}
extension ATOM.MutablePersistableRecord {
  @discardableResult
  public static func deleteAll<Sequence>(_ db: ATOM.Database, keys: Sequence) throws -> Swift.Int where Sequence : Swift.Sequence, Sequence.Element : ATOM.DatabaseValueConvertible
  @discardableResult
  public static func deleteOne<PrimaryKeyType>(_ db: ATOM.Database, key: PrimaryKeyType?) throws -> Swift.Bool where PrimaryKeyType : ATOM.DatabaseValueConvertible
}
extension ATOM.MutablePersistableRecord {
  @discardableResult
  public static func deleteAll(_ db: ATOM.Database, keys: [[Swift.String : (any ATOM.DatabaseValueConvertible)?]]) throws -> Swift.Int
  @discardableResult
  public static func deleteOne(_ db: ATOM.Database, key: [Swift.String : (any ATOM.DatabaseValueConvertible)?]) throws -> Swift.Bool
}
public protocol PersistableRecord : ATOM.MutablePersistableRecord {
  func didInsert(with rowID: Swift.Int64, for column: Swift.String?)
  func insert(_ db: ATOM.Database) throws
  func save(_ db: ATOM.Database) throws
}
extension ATOM.PersistableRecord {
  public func didInsert(with rowID: Swift.Int64, for column: Swift.String?)
  public func insert(_ db: ATOM.Database) throws
  public func save(_ db: ATOM.Database) throws
  @inlinable public func performInsert(_ db: ATOM.Database) throws {
        let conflictResolutionForInsert = type(of: self).persistenceConflictPolicy.conflictResolutionForInsert
        let dao = try DAO(db, self)
        try dao.insertStatement(onConflict: conflictResolutionForInsert).execute()
        
        if !conflictResolutionForInsert.invalidatesLastInsertedRowID {
            didInsert(with: db.lastInsertedRowID, for: dao.primaryKey.rowIDColumn)
        }
    }
  @inlinable public func performSave(_ db: ATOM.Database) throws {
        // Call self.insert and self.update so that we support classes that
        // override those methods.
        if let key = try primaryKey(db) {
            do {
                try update(db)
            } catch PersistenceError.recordNotFound(databaseTableName: type(of: self).databaseTableName, key: key) {
                try insert(db)
            }
        } else {
            try insert(db)
        }
    }
}
@usableFromInline
final internal class DAO<Record> where Record : ATOM.MutablePersistableRecord {
  @usableFromInline
  final internal let primaryKey: ATOM.PrimaryKeyInfo
  @usableFromInline
  internal init(_ db: ATOM.Database, _ record: Record) throws
  @usableFromInline
  final internal func insertStatement(onConflict: ATOM.Database.ConflictResolution) throws -> ATOM.UpdateStatement
  @usableFromInline
  final internal func updateStatement(columns: Swift.Set<Swift.String>, onConflict: ATOM.Database.ConflictResolution) throws -> ATOM.UpdateStatement?
  @usableFromInline
  final internal func deleteStatement() throws -> ATOM.UpdateStatement?
  @usableFromInline
  final internal func existsStatement() throws -> ATOM.SelectStatement?
  @usableFromInline
  final internal func makeRecordNotFoundError() -> any Swift.Error
  @objc @usableFromInline
  deinit
}
