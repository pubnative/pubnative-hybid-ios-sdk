  final public func next() throws -> Base.Element?
  public typealias Element = Base.Element
  @objc deinit
}
@_hasMissingDesignatedInitializers final public class DropWhileCursor<Base> : ATOM.Cursor where Base : ATOM.Cursor {
  final public func next() throws -> Base.Element?
  public typealias Element = Base.Element
  @objc deinit
}
@_hasMissingDesignatedInitializers final public class EnumeratedCursor<Base> : ATOM.Cursor where Base : ATOM.Cursor {
  final public func next() throws -> (Swift.Int, Base.Element)?
  public typealias Element = (Swift.Int, Base.Element)
  @objc deinit
}
@_hasMissingDesignatedInitializers final public class FilterCursor<Base> : ATOM.Cursor where Base : ATOM.Cursor {
  final public func next() throws -> Base.Element?
  public typealias Element = Base.Element
  @objc deinit
}
@_hasMissingDesignatedInitializers final public class FlattenCursor<Base> : ATOM.Cursor where Base : ATOM.Cursor, Base.Element : ATOM.Cursor {
  final public func next() throws -> Base.Element.Element?
  public typealias Element = Base.Element.Element
  @objc deinit
}
@_hasMissingDesignatedInitializers final public class MapCursor<Base, Element> : ATOM.Cursor where Base : ATOM.Cursor {
  final public func next() throws -> Element?
  @objc deinit
}
@_hasMissingDesignatedInitializers final public class PrefixCursor<Base> : ATOM.Cursor where Base : ATOM.Cursor {
  final public func next() throws -> Base.Element?
  public typealias Element = Base.Element
  @objc deinit
}
@_hasMissingDesignatedInitializers final public class PrefixWhileCursor<Base> : ATOM.Cursor where Base : ATOM.Cursor {
  final public func next() throws -> Base.Element?
  public typealias Element = Base.Element
  @objc deinit
}
public struct PreparedRequest {
  public var statement: ATOM.SelectStatement
  public var adapter: (any ATOM.RowAdapter)?
  public init(statement: ATOM.SelectStatement, adapter: (any ATOM.RowAdapter)? = nil)
}
public protocol FetchRequest : ATOM.DatabaseRegionConvertible {
  associatedtype RowDecoder
  func prepare(_ db: ATOM.Database, forSingleResult singleResult: Swift.Bool) throws -> (ATOM.SelectStatement, (any ATOM.RowAdapter)?)
  func makePreparedRequest(_ db: ATOM.Database, forSingleResult singleResult: Swift.Bool) throws -> ATOM.PreparedRequest
  func fetchCount(_ db: ATOM.Database) throws -> Swift.Int
}
extension ATOM.FetchRequest {
  public func adapted(_ adapter: @escaping (ATOM.Database) throws -> any ATOM.RowAdapter) -> ATOM.AdaptedFetchRequest<Self>
  public func fetchCount(_ db: ATOM.Database) throws -> Swift.Int
  public func databaseRegion(_ db: ATOM.Database) throws -> ATOM.DatabaseRegion
  public func makePreparedRequest(_ db: ATOM.Database, forSingleResult singleResult: Swift.Bool) throws -> ATOM.PreparedRequest
  public func prepare(_ db: ATOM.Database, forSingleResult singleResult: Swift.Bool) throws -> (ATOM.SelectStatement, (any ATOM.RowAdapter)?)
}
public struct AdaptedFetchRequest<Base> : ATOM.FetchRequest where Base : ATOM.FetchRequest {
  public typealias RowDecoder = Base.RowDecoder
  public func makePreparedRequest(_ db: ATOM.Database, forSingleResult singleResult: Swift.Bool) throws -> ATOM.PreparedRequest
  public func fetchCount(_ db: ATOM.Database) throws -> Swift.Int
  public func databaseRegion(_ db: ATOM.Database) throws -> ATOM.DatabaseRegion
}
public struct AnyFetchRequest<T> : ATOM.FetchRequest {
  public typealias RowDecoder = T
  public init<Request>(_ request: Request) where Request : ATOM.FetchRequest
  @available(*, deprecated, message: "Define your own FetchRequest type instead.")
  public init(_ prepare: @escaping (ATOM.Database, _ singleResult: Swift.Bool) throws -> (ATOM.SelectStatement, (any ATOM.RowAdapter)?))
  public func makePreparedRequest(_ db: ATOM.Database, forSingleResult singleResult: Swift.Bool) throws -> ATOM.PreparedRequest
  public func fetchCount(_ db: ATOM.Database) throws -> Swift.Int
  public func databaseRegion(_ db: ATOM.Database) throws -> ATOM.DatabaseRegion
}
