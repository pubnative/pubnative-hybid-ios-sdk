public protocol DatabaseWriter : ATOM.DatabaseReader {
  func write<T>(_ updates: (ATOM.Database) throws -> T) throws -> T
  func writeWithoutTransaction<T>(_ updates: (ATOM.Database) throws -> T) rethrows -> T
  func barrierWriteWithoutTransaction<T>(_ updates: (ATOM.Database) throws -> T) rethrows -> T
  func asyncWrite<T>(_ updates: @escaping (ATOM.Database) throws -> T, completion: @escaping (ATOM.Database, Swift.Result<T, any Swift.Error>) -> Swift.Void)
  func asyncWriteWithoutTransaction(_ updates: @escaping (ATOM.Database) -> Swift.Void)
  func unsafeReentrantWrite<T>(_ updates: (ATOM.Database) throws -> T) rethrows -> T
  func concurrentRead<T>(_ block: @escaping (ATOM.Database) throws -> T) -> ATOM.DatabaseFuture<T>
  func spawnConcurrentRead(_ block: @escaping (Swift.Result<ATOM.Database, any Swift.Error>) -> Swift.Void)
}
extension ATOM.DatabaseWriter {
  public func write<T>(_ updates: (ATOM.Database) throws -> T) throws -> T
  public func asyncWrite<T>(_ updates: @escaping (ATOM.Database) throws -> T, completion: @escaping (ATOM.Database, Swift.Result<T, any Swift.Error>) -> Swift.Void)
  public func add(transactionObserver: any ATOM.TransactionObserver, extent: ATOM.Database.TransactionObservationExtent = .observerLifetime)
  public func remove(transactionObserver: any ATOM.TransactionObserver)
  public func erase() throws
  public func vacuum() throws
  public func add<Reducer>(observation: ATOM.ValueObservation<Reducer>, onError: @escaping (any Swift.Error) -> Swift.Void, onChange: @escaping (Reducer.Value) -> Swift.Void) -> any ATOM.TransactionObserver where Reducer : ATOM.ValueReducer
}
@_hasMissingDesignatedInitializers public class DatabaseFuture<Value> {
  public func wait() throws -> Value
  @objc deinit
}
