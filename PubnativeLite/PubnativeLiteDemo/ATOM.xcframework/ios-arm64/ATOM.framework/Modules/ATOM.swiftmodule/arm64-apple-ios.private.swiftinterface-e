  public init(sqliteStatement: ATOM.SQLiteStatement, index: Swift.Int32)
  public var databaseValue: ATOM.DatabaseValue {
    get
  }
  public static func fromDatabaseValue(_ dbValue: ATOM.DatabaseValue) -> ATOM.DatabaseDateComponents?
  public init(from decoder: any Swift.Decoder) throws
  public func encode(to encoder: any Swift.Encoder) throws
}
